{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.local/fb01_patch_manifest.schema.json",
  "title": "FB01 Patch Library Manifest",
  "type": "object",
  "required": [
    "generated_utc",
    "count",
    "items"
  ],
  "properties": {
    "generated_utc": {
      "type": "string"
    },
    "count": {
      "type": "integer",
      "minimum": 0
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "path",
          "bytes",
          "sha1",
          "header_hex",
          "tail_hex",
          "folder",
          "name"
        ],
        "properties": {
          "path": {
            "type": "string"
          },
          "bytes": {
            "type": "integer",
            "minimum": 1
          },
          "sha1": {
            "type": "string",
            "pattern": "^[a-f0-9]{40}$"
          },
          "header_hex": {
            "type": "string"
          },
          "tail_hex": {
            "type": "string"
          },
          "folder": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}